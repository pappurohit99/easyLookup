{
  "name": "EasyLookup",
  "version": "1.0",
  "description": "Search MATWORKS Documentations for the function",
  "permissions": ["storage","activeTab"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/EasyLookup16.png",
      "32": "images/EasyLookup32.png",
      "48": "images/EasyLookup48.png",
      "128": "images/EasyLookup128.png"
    }
  },
  "icons": {
    "16": "images/EasyLookup16.png",
    "32": "images/EasyLookup32.png",
    "48": "images/EasyLookup48.png",
    "128": "images/EasyLookup128.png"
  },
  "content_scripts": [
     {
         "matches": [
             "<all_urls>"
         ],
         "js": ["content.js"]
     }
 ],
  "manifest_version": 2
}